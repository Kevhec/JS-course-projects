{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "./node_modules/ts-standard/eslintrc.json"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./Front-end/tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/no-misused-promises": [2, {
        "checksVoidReturn": {
          "attributes": false
        }
      }]
    }
}
